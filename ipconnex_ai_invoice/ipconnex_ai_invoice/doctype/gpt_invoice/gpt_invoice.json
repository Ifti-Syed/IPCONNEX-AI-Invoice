      "options":"Company"
      },     
      {
        "fieldname": "gpt_account",
        "fieldtype": "Link",
        "label": "GPT Account",
        "options":"GPT Account"
      },      
      {
        "fieldname": "invoice_file",
        "fieldtype": "Attach",
        "label": "Join an Invoice File"
      },   
      {
        "fieldname": "extract_data",
        "fieldtype": "Button",
        "label": "Extract Data"
      },  
      {
         "fieldname": "invoice_date",
         "fieldtype": "Date",
         "label": "Invoice Date"
      },   
      {
       "fieldname": "invoice_type",
       "fieldtype": "Select",
       "label": "Type",
       "options":"Purchase\nSales"
      }, 
      {
        "fieldname": "customer",
        "fieldtype": "Link",
        "label": "Customer",
        "options":"Customer",
        "depends_on": "eval:doc.invoice_type=='Sales'"
      }, 
      {
        "fieldname": "Supplier",
        "fieldtype": "Link",
        "label": "Supplier",
        "options":"Supplier",
        "depends_on": "eval:doc.invoice_type=='Purchase'"
      }, 
      {
        "fieldname": "invoice_items",
        "fieldtype": "Table",
        "label": "Items",
        "options":"GPT Invoice Item"
      },
      {
        "fieldname": "generate_invoice",
        "fieldtype": "Button",
        "label": "Generate Invoice",
        "depends_on": "eval:doc.invoice_items && doc.invoice_date && !doc.generated_sales && !doc.generated_purchase"
      }, 
      {
        "fieldname": "generated_sales",
        "fieldtype": "Link",
        "label": "Generated Sales",
        "options":"Sales Invoice",
        "depends_on": "eval:doc.generated_sales"
      },        
      {
        "fieldname": "generated_purchase",
        "fieldtype": "Link",
        "label": "Generated Purchase",
        "options":"Purchase Invoice",
        "depends_on": "eval:doc.generated_purchase"
      }


