{
  "actions": [],
  "allow_rename": 1,
  "autoname": "format:GPT-Invoice-{YYYY}-{######}",
  "creation": "2023-12-09 00:00:00",
  "doctype": "DocType",
  "editable_grid": 1,
  "engine": "InnoDB",
  "field_order": [
    "gpt_account",
    "company",
    "invoice_file",
    "invoice_type",
    "invoice_default_item",
    "extract_data",
    "customer_name",
    "supplier_name",
    "income_account",
    "currency",
    "invoice_date",
    "mode_of_payment",
    "invoice_items",
    "invoice_total_amount",
    "extracted_amount",
    "difference",
    "generate_invoice",
    "generated_sales",
    "generated_purchase"
  ],
  "fields": [
    {
      "fieldname": "gpt_account",
      "fieldtype": "Link",
      "label": "GPT Setting",
      "options": "GPT Setting"
    },
    {
      "fieldname": "company",
      "fieldtype": "Link",
      "label": "Company",
      "options": "Company"
    },
    {
      "fieldname": "invoice_file",
      "fieldtype": "Attach",
      "label": "Join an Invoice File"
    },
    {
      "fieldname": "invoice_type",
      "fieldtype": "Select",
      "label": "Type",
      "options": "Purchase\nSales"
    },
    {
      "fieldname": "invoice_default_item",
      "fieldtype": "Link",
      "label": "Default Item",
      "options": "Item"
    },
    {
      "depends_on": "eval:doc.invoice_file && doc.gpt_account && (!doc.invoice_date || !doc.invoice_items || doc.invoice_items.length==0)",
      "fieldname": "extract_data",
      "fieldtype": "Button",
      "label": "Extract Data"
    },
    {
      "depends_on": "eval:doc.invoice_type=='Sales'",
      "fieldname": "customer_name",
      "fieldtype": "Link",
      "label": "Customer",
      "options": "Customer"
    },
    {
      "depends_on": "eval:doc.invoice_type=='Purchase'",
      "fieldname": "supplier_name",
      "fieldtype": "Link",
      "label": "Supplier",
      "options": "Supplier"
    },
    {
      "depends_on": "eval:doc.invoice_type=='Sales'",
      "fieldname": "income_account",
      "fieldtype": "Link",
      "label": "Income Account",
      "options": "Account"
    },
    {
      "depends_on": "eval:doc.company",
      "fieldname": "currency",
      "fieldtype": "Link",
      "label": "Currency",
      "options": "Currency"
    },
    {
      "fieldname": "invoice_date",
      "fieldtype": "Date",
      "label": "Invoice Date"
    },
    {
      "fieldname": "mode_of_payment",
      "fieldtype": "Link",
      "label": "Mode of Payment",
      "options": "Mode of Payment"
    },
    {
      "fieldname": "invoice_items",
      "fieldtype": "Table",
      "label": "Items",
      "options": "Invoice Import Tool Item"
    },
    {
      "fieldname": "invoice_total_amount",
      "fieldtype": "Currency",
      "label": "Total Amount",
      "read_only": 1
    },
    {
      "fieldname": "extracted_amount",
      "fieldtype": "Currency",
      "label": "Extracted Amount",
      "read_only": 1
    },
    {
      "fieldname": "difference",
      "fieldtype": "Currency",
      "label": "Difference",
      "read_only": 1
    },
    {
      "depends_on": "eval:doc.invoice_date && !doc.generated_sales && !doc.generated_purchase",
      "fieldname": "generate_invoice",
      "fieldtype": "Button",
      "label": "Generate Invoice"
    },
    {
      "depends_on": "eval:doc.generated_sales",
      "fieldname": "generated_sales",
      "fieldtype": "Link",
      "label": "Generated Sales",
      "options": "Sales Invoice"
    },
    {
      "depends_on": "eval:doc.generated_purchase",
      "fieldname": "generated_purchase",
      "fieldtype": "Link",
      "label": "Generated Purchase",
      "options": "Purchase Invoice"
    }
  ],
  "links": [],
  "module": "IPCONNEX AI Invoice",
  "name": "Invoice Import Tool",
  "naming_rule": "Expression",
  "owner": "Administrator",
  "permissions": [
    {
      "role": "System Manager",
      "read": 1,
      "write": 1,
      "create": 1,
      "delete": 1,
      "print": 1,
      "email": 1,
      "export": 1,
      "share": 1,
      "report": 1
    }
  ],
  "quick_entry": 1,
  "row_format": "Dynamic",
  "sort_field": "modified",
  "sort_order": "DESC"
}