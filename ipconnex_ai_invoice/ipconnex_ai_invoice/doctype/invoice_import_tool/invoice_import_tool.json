      {
        "fieldname": "supplier_name",
        "fieldtype": "Link",
        "label": "Supplier",
        "options":"Supplier",
        "depends_on": "eval:doc.invoice_type=='Purchase'"
      }, 
      {
        "fieldname": "currency",
        "fieldtype": "Link",
        "label": "Currency",
        "options":"Currency",
        "depends_on": "eval:doc.company"
      }, 
      {
        "fieldname": "invoice_items",
        "fieldtype": "Table",
        "label": "Items",
        "options":"Invoice Import Tool Item"
      },
      {
        "fieldname": "invoice_total_amount",
        "fieldtype": "Currency",
        "label": "Total Amount"
      },
      {
        "fieldname": "generate_invoice",
        "fieldtype": "Button",
        "label": "Generate Invoice",
        "depends_on": "eval:doc.invoice_date && !doc.generated_sales && !doc.generated_purchase"
      }, 
      {
        "fieldname": "generated_sales",
        "fieldtype": "Link",
        "label": "Generated Sales",
        "options":"Sales Invoice",
        "depends_on": "eval:doc.generated_sales"
      },        
      {
        "fieldname": "generated_purchase",
        "fieldtype": "Link",
        "label": "Generated Purchase",
        "options":"Purchase Invoice",
        "depends_on": "eval:doc.generated_purchase"
      }
